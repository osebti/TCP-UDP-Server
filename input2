# Transactions file for one client
# a3p1 -s 2222
#    a3p1 -c 1 a3p1-w24-ex1.dat ug03.cs.ualberta.ca 2222
1 gtime
1 put   video1.mp4
{
   video1.mp4: line 1
   video1.mp4: line 2
   video1.mp4: line 3
}
1 put   img1.jpg
{
   img1.jpg: line 1
   img1.jpg: line 2
}
1 put   index1.html
{
   index1.html: line 1
}
1 put doc1.tex
{
}
1 delay 2500
1 gtime
1 get  video1.mp4
1 get  img1.jpg
1 get    index2.html
1 delete img2.jpg
1 delay  3000
1 gtime 1 quit
